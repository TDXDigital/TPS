{
    "name": "tps/slim-mvc",
    "description": "TPS system utilities",
    "repositories": {
      "treant-js": {
        "type": "package",
        "package": {
          "name": "fperucic/treant-js",
          "version": "1.0.0",
          "source": {
            "url": "https://github.com/fperucic/treant-js",
            "type": "git",
            "reference": "master"
          }
        }
      }
    },
    "require": {
        "slim/views": "^0.1.3",
        "slim/extras": "*",
        "twig/twig": "^1.18",
        "slim/slim": "2.*",
        "twbs/bootstrap": "^3.3",
        "smarty/smarty": "^3.0",
        "apigen/apigen": "2.8.0",
        "components/jquery": "^1.8",
        "components/font-awesome": "^4.0",
        "nette/nette": "^3.0",
        "fperucic/treant-js": "*",
        "onokumus/metismenu": "dev-master",
        "mk-j/php_xlsxwriter": "*",
        "hellogerard/jobby": "*"
    },
    "require-dev": {
        "phpunit/phpunit-skeleton-generator": "*",
        "phpunit/phpunit": "*",
        "codacy/coverage": "dev-master",
        "phpunit/dbunit": "2.0.3"
    },
    "autoload":{
        "psr-0":{
            "TPS\\": ["router/", "public/lib/"],
            "Controller\\": "./"
        }
    },
    "license": "MIT",
    "authors": [
        {
            "name": "James Oliver",
            "email": "j.oliver@alumni.uleth.ca"
        }
    ]
}
